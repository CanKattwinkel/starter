{
  "name": "parkspot",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot --locale en-GB --proxy-config proxy.conf.json",
    "start:de": "ng serve --aot --locale en-GB --i18n-file=./src/locale/messages.de-DE.xlf --proxy-config proxy.conf.json ",
    "start:fr": "ng serve --aot --locale fr-CA --i18n-file=./src/locale/messages.fr-CA.xlf --proxy-config proxy.conf.json ",
    "start:es": "ng serve --aot --locale es-ES --i18n-file=./src/locale/messages.es-ES.xlf --proxy-config proxy.conf.json ",
    "build:single:lang": "ng build  --prod --locale en-GB --base-href /en-GB/ --output-path dist/en-GB",
    "prebuild": "npm run build:clean && npm run i18n &&  npm run xliffmerge",
    "build": "npm run build:single:lang  && for lang in de-DE fr-CA es-ES; do LANG=$lang npm run build:single:lang -- --locale $lang  --base-href /$lang/ --i18n-file=./src/locale/messages.$lang.xlf --output-path dist/$lang;  done",
    "build:clean": "rimraf dist/",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n": "ng xi18n --locale en-GB --output-path src/locale --i18nFormat=xlf",
    "posti18n": "npm run xliffmerge",
    "xliffmerge": "xliffmerge --profile xliffmerge.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "date-fns": "^1.29.0",
    "localforage": "^1.5.0",
    "material-design-icons": "^3.0.1",
    "ngforage": "2.0.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "^2.8.6",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-i18nsupport": "^0.13.0",
    "protractor": "~5.1.2",
    "puppeteer": "^1.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
